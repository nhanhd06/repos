<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Home Page</title>

    <environment include="Development">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link rel="stylesheet" href="~/css/1.css" />
        <link rel="stylesheet" href="~/css/2.css" />
        <link rel="stylesheet" href="~/css/3.css" />
        <link rel="stylesheet" href="~/css/4.css" />
        <link rel="stylesheet" href="~/css/5.css" />

        <link rel="stylesheet" href="~/lib/vendor/font-awesome.css" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    </environment>
    <environment include="Development">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="~/js/1.js"></script>
        <script type="text/javascript" src="~/js/2.js"></script>
    </environment>
</head>
<body>
    <div class="container-fluid">
        <nav class="navbar navbar-inverse navbar-fixed-top">

            <div class="container-fluid">
                <div class="navbar-header">

                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Homepage">Website</a>
                </div>
                <div class="collapse navbar-collapse" id="myNavbar">
                    <ul class="nav navbar-nav">
                        <li class="home"><a asp-area="" asp-controller="Home" asp-action="Homepage">Home</a></li>
                        <li class="about"><a asp-area="" asp-controller="Home" asp-action="Education">Education</a></li>
                        <li class="work"><a asp-area="" asp-controller="Home" asp-action="Experience">Work Experience</a></li>
                        <li class="lives"><a asp-area="" asp-controller="Home" asp-action="Skills">Skills</a></li>
                    </ul>

                </div>
            </div>
        </nav>

        @RenderBody()

        <div class="container-fluid footerSection">
            <div class="row">
                <div class="col-sm-12">
                    <div class="row">
                        <div class="col-sm-1" style="margin-right: 52px;">
                        </div>
                        <div class="col-sm-7">

                            <div class="col-sm-3 menu1">
                                <a asp-area="" asp-controller="Home" asp-action="Homepage">
                                    <i class="fas fa-caret-right mb-1"><strong>&nbsp;&nbsp;&nbsp;&nbsp;HOME</strong></i>
                                </a>

                            </div>

                            <div class="col-sm-3 menu1">
                                <a asp-area="" asp-controller="Home" asp-action="Education">
                                    <i class="fas fa-caret-right mb-1"><strong>&nbsp;&nbsp;&nbsp;&nbsp;EDUCATION</strong></i>
                                </a>
                            </div>
                            <div class="col-sm-3 menu1">
                                <a asp-area="" asp-controller="Home" asp-action="Experience">
                                    <i class="fas fa-caret-right mb-1"><strong>&nbsp;&nbsp;&nbsp;&nbsp;WORK EXPERIENCE</strong></i>
                                </a>
                            </div>

                            <div class="col-sm-3 menu1">
                                <a asp-area="" asp-controller="Home" asp-action="Skills">
               
                                    <i class="fas fa-caret-right mb-1"><strong>&nbsp;&nbsp;&nbsp;&nbsp;SKILLS</strong></i>
                                </a>
                            </div>
                        </div>


                        <div class="col-sm-2">
                            <div class="row">
                                <ul class="list-inline">
                                    <li class="list-inline-item">
                                        <a href="#">
                                            <span class="icon">
                                                <i class="fab fa-twitter"></i>
                                            </span>
                                        </a>
                                    </li>
                                    <li class="list-inline-item">
                                        <a href="#">
                                            <span class="icon">
                                                <i class="fab fa-facebook"></i>
                                            </span>
                                        </a>
                                    </li>
                                    <li class="list-inline-item">
                                        <a href="#">
                                            <span class="icon">
                                                <i class="fab fa-google-plus-g"></i>
                                            </span>
                                        </a>
                                    </li>
                                </ul>
                            </div>

                        </div>

                        <div class="col-sm-1">
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="container-fluid footerSection2">
            <div class="row">
                <div class="col-sm-12">
                    <div class="row">
                        <div class="col-sm-1" style="margin-right: 52px;">
                        </div>
                        <div class="col-sm-3">
                            <p class="title"><strong>All Rights Reserved To Nhan Dao 2019</strong></p>
                        </div>
                        <div class="col-sm-1">
                        </div>

                        <div class="col-sm-6">

                            <div class="col-sm-5">
                                <p class="title" style="font-size: 18px"><strong>Contact Me</strong></p>
                                <span class="footer2Content">
                                    <i class="fas fa-map-marker-alt mr-1 mb-2"></i>179 N 5th Apt 8 San Jose CA
                                </span>
                                <br>
                                <span class="footer2Content">
                                    <i class="fas fa-phone mr-1 mb-2"></i>408-721-9820
                                </span>
                                <br>
                                <span class="footer2Content">
                                    <i class="fas fa-envelope-square mr-1 mb-2"></i>nhandao31@gmail.com
                                </span>
                            </div>
                            <div class="col-sm-7">
                                <div id="map-container-google-1" class="z-depth-1-half map-container">
                                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3172.078336031171!2d-121.89075708469488!3d37.34065407984077!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x808fcc97c6c72a93%3A0x7cefa47c72013125!2s179+N+5th+St+%238%2C+San+Jose%2C+CA+95112!5e0!3m2!1sen!2sus!4v1555709345965!5m2!1sen!2sus" width="370" height="150" frameborder="0" style="border:0" allowfullscreen></iframe>
                                </div>
                            </div>

                        </div>
                        <div class="col-sm-1" style="margin-left: 52px;">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>




</body>
</html>
